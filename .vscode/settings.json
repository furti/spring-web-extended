{
    "java.sources.organizeImports.staticStarThreshold": 1,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.codeGeneration.hashCodeEquals.useJava7Objects": true,
    "java.codeGeneration.toString.codeStyle": "STRING_FORMAT",
    "java.debug.settings.onBuildFailureProceed": true,
    "java.cleanup.actions": [
        "renameFileToType",
        "addOverride",
        "addDeprecated",
        "invertEquals",
        "instanceofPatternMatch",
        "switchExpression",
        "tryWithResource",
        "lambdaExpression",
        "useSwitchForInstanceofPattern",
        "lambdaExpressionFromAnonymousClass",
        "lambdaExpression",
        "redundantComparisonStatement",
        "redundantFallingThroughBlockEnd",
        "redundantIfCondition",
        "redundantModifiers",
        "redundantSuperCall"
    ]
}
